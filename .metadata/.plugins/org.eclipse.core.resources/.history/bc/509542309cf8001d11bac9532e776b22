package rough;

import java.util.Scanner;

public class ArmsStrongNum {

	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		
		System.out.println("Enter an number");
		int a=sc.nextInt();
		int pro=1;
		
		while(a>0)
		{
			pro =pro*10+a/10;
			//a=a%10;
		}
		
		System.out.println(a);
		
		
		

	}

}
